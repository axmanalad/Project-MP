{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@prisma": ["./generated/prisma"],
    },
    "moduleResolution": "node",
    "target": "es6",
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*", "prisma/**/*"],
  "exclude": ["node_modules", "../frontend", "../frontend/**/*"]
}
